<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yunfenghui.erp.order.dao.OrderMessageDao">
	<insert id="insertOrderMessage" parameterType="com.yunfenghui.erp.order.model.OrderMessage">
		INSERT INTO order_message(order_no, create_time)
